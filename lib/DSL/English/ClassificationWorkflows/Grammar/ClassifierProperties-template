        <performance-goal-prop> |
        <probabilities-function-prop> |
        <properties-prop> |
        <training-class-priors-prop> |
        <training-time-prop> |
        <utility-function-prop>}

    rule acceptance-threshold-prop {<acceptance-word> <threshold-word> | 'AcceptanceThreshold'}
    rule accuracy-prop {<accuracy-word> | 'Accuracy'}
    rule anomaly-detector-prop {<anomaly-word> <detector-word> | 'AnomalyDetector'}
    rule batch-evaluation-speed-prop {<batch-word> <evaluation-word> <speed-word> | 'BatchEvaluationSpeed'}
    rule batch-evaluation-time-prop {<batch-word> <evaluation-word> <time-word> | 'BatchEvaluationTime'}
    rule classes-prop {<classes-word> | 'Classes'}
    rule class-number-prop {<class-word> <number-word> | 'ClassNumber'}
    rule class-priors-prop {<class-word> <priors-word> | 'ClassPriors'}
    rule evaluation-time-prop {<evaluation-word> <time-word> | 'EvaluationTime'}
    rule example-number-prop {<example-word> <number-word> | 'ExampleNumber'}
    rule feature-names-prop {<feature-word> <names-word> | 'FeatureNames'}
    rule feature-number-prop {<feature-word> <number-word> | 'FeatureNumber'}
    rule feature-types-prop {<feature-word> <types-word> | 'FeatureTypes'}
    rule function-prop {<function-word> | 'Function'}
    rule function-memory-prop {<function-word> <memory-word> | 'FunctionMemory'}
    rule function-properties-prop {<function-word> <properties-word> | 'FunctionProperties'}
    rule indeterminate-threshold-prop {<indeterminate-word> <threshold-word> | 'IndeterminateThreshold'}
    rule l1-regularization-prop {<l1-word> <regularization-word> | 'L1Regularization'}
    rule l2-regularization-prop {<l2-word> <regularization-word> | 'L2Regularization'}
    rule learning-curve-prop {<learning-word> <curve-word> | 'LearningCurve'}
    rule max-training-memory-prop {<max-word> <training-word> <memory-word> | 'MaxTrainingMemory'}
    rule mean-cross-entropy-prop {<mean-word> <cross-word> <entropy-word> | 'MeanCrossEntropy'}
    rule method-prop {<method-word> | 'Method'}
    rule method-description-prop {<method-word> <description-word> | 'MethodDescription'}
    rule method-option-prop {<method-word> <option-word> | 'MethodOption'}
    rule optimization-method-prop {<optimization-word> <method-word> | 'OptimizationMethod'}
    rule performance-goal-prop {<performance-word> <goal-word> | 'PerformanceGoal'}
    rule probabilities-function-prop {<probabilities-word> <function-word> | 'ProbabilitiesFunction'}
    rule properties-prop {<properties-word> | 'Properties'}
    rule training-class-priors-prop {<training-word> <class-word> <priors-word> | 'TrainingClassPriors'}
    rule training-time-prop {<training-word> <time-word> | 'TrainingTime'}
    rule utility-function-prop {<utility-word> <function-word> | 'UtilityFunction'}

    token acceptance-word {'acceptance'}
